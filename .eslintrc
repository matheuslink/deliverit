{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "plugins": ["react"],
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "class-methods-use-this": 0,
    "react/jsx-uses-vars": 2,
    "max-len": [2, 100, 4, {"ignoreUrls": true}]
  }
}